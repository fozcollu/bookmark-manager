{
  "name": "Fozcollu - Bookmark Manager",
  "version": "1.1",
  "description": "Search, Add, edit, delete bookmarks",
  "permissions": ["bookmarks", "storage", "chrome://favicon/"],
  "browser_action": {
    "default_title": "Bookmark Manager",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
